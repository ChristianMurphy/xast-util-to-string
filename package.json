{
  "name": "xast-util-to-string",
  "version": "1.0.0",
  "description": "xast utility to get the text value of a node",
  "license": "MIT",
  "keywords": [
    "unist",
    "xast",
    "xast-util",
    "util",
    "utility",
    "xml",
    "string",
    "content",
    "text"
  ],
  "repository": "syntax-tree/xast-util-to-string",
  "bugs": "https://github.com/syntax-tree/xast-util-to-string/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "unist-builder": "^2.0.0",
    "xo": "^0.38.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . --write --loglevel warn && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
